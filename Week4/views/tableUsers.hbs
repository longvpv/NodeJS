<div id="content">

    <div class="container-fluid">

        <h1 class="h3 mb-2 text-gray-800">Users Table</h1>

        <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-primary">Users Table Example</h6>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th>First Name</th>
                                <th>Last Name</th>
                                <th>Date of birth</th>
                                <th>Gender</th>
                                <th>Country</th>
                                <th>Phone Number</th>
                                <th>User Name</th>
                                <th>Email</th>
                                <th>Role</th>
                            </tr>
                        </thead>
                        <tfoot>
                            <tr>
                                <th>First Name</th>
                                <th>Last Name</th>
                                <th>Date of birth</th>
                                <th>Gender</th>
                                <th>Country</th>
                                <th>Phone Number</th>
                                <th>User Name</th>
                                <th>Email</th>
                                <th>Role</th>
                            </tr>
                        </tfoot>
                        <tbody>
                            {{#each users }}
                            <tr>
                                <td>{{firstName}}</td>
                                <td>{{lastName}}</td>
                                <td>{{dob}}</td>
                                <td>{{gender}}</td>
                                <td>{{country}}</td>
                                <td>{{phoneNumber}}</td>
                                <td>{{username}}</td>
                                <td>{{email}}</td>
                                <td>{{role}}</td>
                            </tr>
                            {{else}}
                            <td>No user !</td>
                            {{/each}}

                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>
    <!-- /.container-fluid -->

</div>